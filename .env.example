# Configuration SentinelIQ Harvest
# Copiez ce fichier vers key.env et remplissez les valeurs

# Base de données Supabase
SUPABASE_URL=https://votre-project.supabase.co
SUPABASE_KEY=votre-clé-api

# Serveur de healthcheck pour Coolify
HEALTH_PORT=3000

# Configuration Logs
DEBUG=true
VERBOSE=true

# Configuration Cortex
CORTEX_PORT=3001
CORTEX_HOST=localhost
CORTEX_LOG_LEVEL=INFO
ENABLE_CORTEX_PERF_LOGS=true
ENABLE_CORTEX_MEM_MONITOR=true
ENABLE_SCRAPING_LOGS=true

# Mode d'environnement
NODE_ENV=production
CORTEX_MODE=full

# Webhook (legacy - peut être laissé vide)
WEBHOOK_TOKEN=
